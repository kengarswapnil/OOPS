public class Main {
	public static void reversearray(int arr[])
	{
		    int n = arr.length;
		    
		    int temp [] = new int [n];
		    
		    for(int i=0;i<n;i++)
		    {
		    	   temp[i]=arr[n-1-i];
		    }
		    
		    for(int j=0;j<n;j++)
		    {
		    	 arr[j]=temp[j];
		    }
	}
	public static void main(String[] args) {
		int arr[]={1,2,3,4,5};
		reversearray(arr);
		
		
		for(int i=0;i<arr.length;i++){
		System.out.print(arr[i]);
		}
	}
}