public class Main {
    public static void main(String[] args) {
        Dog d = new Dog();
        System.out.println(d.color);
    }
}


class Animal{
    String color;
    Animal(){
        System.out.println("Animal constructor called....");
    }
}

class Dog extends Animal {
   
    Dog(){
         super.color = "black";
        System.out.println("Dog Constructor called...");
    }
}


